  },
  "properties": {
    "product_code": {
      "type": "string"
    },
    "product_name": {
      "type": "string"
    },
    "product_description": {
      "type": "string"
    },
    "model_id": {
      "type": "string"
    },
    "product_price": {
      "type": "number"
    },
    "average_code": {
      "type": "number"
    },
